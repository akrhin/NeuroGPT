{
    // Ваш OpenAI API Key, обычно обязательно,
    // Если по умолчанию установлено "openai_api_key": "" , то необходимо ввести API Key в графическом интерфейсе
    "openai_api_key": " ",

    "google_palm_api_key": "",
    "xmchat_api_key": "",
    "usage_limit": 999, // Месячный лимит API Key, в долларах США
    // Ваш xmchat API Key, отличается от OpenAI API Key
    // APIKey MiniMax (см. страницу управления учетной записью https://api.minimax.chat/basic-information) и Group ID, используемые для модели диалога MiniMax
    "minimax_api_key": "",
    "minimax_group_id": "",
    "language": "ru_RU",
    // Поддерживает настройку OpenAI API Base
    "openai_api_base": "https://chimeragpt.adventblocks.cc",
    // Если используется прокси, отмените комментирование следующих двух строк и замените URL прокси
    // "https_proxy": "http://127.0.0.1:1079",
    // "http_proxy": "http://127.0.0.1:1079",
    "users": [], // Список пользователей, [[имя пользователя1, пароль1], [имя пользователя2, пароль2], ...]
    "local_embedding": false, //Создавать ли индекс локально
    "hide_history_when_not_logged_in": false, //Скрыть ли историю диалогов, когда пользователь не вошел в систему
    "check_update": false, //Включить ли проверку обновлений
    "default_model": "gpt-3.5-turbo", // Модель по умолчанию

    //Настройки помощника Чуаньху
    "default_chuanhu_assistant_model": "gpt-4", //Модель, используемая помощником Чуаньху, можно выбрать gpt-3.5-turbo или gpt-4
    "GOOGLE_CSE_ID": "", //ID поискового движка Google, используется в режиме Pro помощника Чуаньху, способ получения смотрите здесь https://stackoverflow.com/questions/37083058/programmatically-searching-google-in-python-using-custom-search
    "GOOGLE_API_KEY": "", //Google API Key, используется в режиме Pro помощника Чуаньху
    "WOLFRAM_ALPHA_APPID": "", //Wolfram Alpha API Key, используется в режиме Pro помощника Чуаньху, способ получения смотрите здесь https://products.wolframalpha.com/api/
    "SERPAPI_API_KEY": "", //SerpAPI API Key, используется в режиме Pro помощника Чуаньху, способ получения смотрите здесь https://serpapi.com/

    "advance_docs": {
        "pdf": {
            // Считать ли PDF двухколоночным
            "two_column": false,
            // Использовать ли OCR для распознавания формул в PDF
            "formula_ocr": true
        }
    },
    // Использовать ли несколько API Key по очереди
    "multi_api_key": false,
    "api_key_list": [
        "sk-xxxxxxxxxxxxxxxxxxxxxxxx1",
        "sk-xxxxxxxxxxxxxxxxxxxxxxxx2",
        "sk-xxxxxxxxxxxxxxxxxxxxxxxx3"
    ],
    // Если используется пользовательский порт или IP, отмените комментирование и замените соответствующий контент
     "server_name": "0.0.0.0",
    // "server_port": 7860,
    // Если хотите поделиться
     "share": false,
}
